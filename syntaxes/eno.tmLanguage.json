			"captures": {
				"1": { "name": "punctuation.definition.section1.operator.eno" },
				"2": { "name": "punctuation.definition.section1.escape.begin.operator.eno" },
				"3": { "name": "variable.other.section1.name.eno" },
				"4": { "name": "punctuation.definition.section1.escape.end.operator.eno" },
				"5": { "name": "punctuation.definition.section1.copy.operator.eno" },
				"6": { "name": "variable.other.section1.template.name.eno" }
			},
			"patterns": []
