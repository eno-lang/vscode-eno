			},
			"patterns": []
		},
		"escapedField": {
			"comment": "`name`: value",
			"match": "^\\s*(`+)\\s*((?:(?!\\1).)+)\\s*(\\1)\\s*(:)\\s*(\\S.*?)\\s*$",
			"captures": {
				"1": { "name": "punctuation.definition.escape.begin.operator.eno" },
				"2": { "name": "variable.other.name.eno" },
				"3": { "name": "punctuation.definition.escape.end.operator.eno" },
				"4": { "name": "punctuation.definition.name.operator.eno" },
				"5": { "name": "string.unquoted.value.eno" }
			},
			"patterns": []
		},
		"escapedName": {
			"comment": "`name`:",
			"match": "^\\s*(`+)\\s*((?:(?!\\1).)+)\\s*(\\1)\\s*(:)\\s*$",
