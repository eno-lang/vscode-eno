				"1": { "name": "invalid.illegal.unspecified.eno" }
			},
			"patterns": []
		},
		"item": {
			"comment": "- value",
			"match": "^\\s*(-)(?!-)\\s*(.+?)?\\s*$",
			"captures": {
				"1": { "name": "punctuation.definition.item.operator.eno" },
				"2": { "name": "string.unquoted.value.eno" }
			},
