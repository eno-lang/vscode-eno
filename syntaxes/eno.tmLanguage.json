	"repository": {
		"block": {
			"comment": "-- name",
			"begin": "^\\s*(-{2,})(?!-)\\s*(.+?)\\s*$",
			"beginCaptures": {
				"1": { "name": "punctuation.definition.block.begin.operator.eno" },
				"2": { "name": "variable.other.block.name.eno" }
			},
			"contentName": "string.unquoted.value.eno",
			"end": "^\\s*(\\1)\\s*(\\2)\\s*$",
			"endCaptures": {
				"1": { "name": "punctuation.definition.block.end.operator.eno" },
				"2": { "name": "variable.other.block.name.eno" }
			},
			"patterns": []
		},
		"comment": {
			"comment": "> comment",
			"match": "^\\s*(>).*$",
			"name": "comment.line.eno",
			"captures": {
				"1": { "name": "punctuation.definition.comment.operator.eno" }
			},
			"patterns": []
		},
		"continuation": {
			"comment": "| value OR \\ value",
			"match": "^\\s*([\\\\|])(?![\\\\|])\\s*(.+?)?\\s*$",
			"captures": {
				"1": { "name": "punctuation.definition.continuation.operator.eno" },
				"2": { "name": "string.unquoted.value.eno" }
			},
			"patterns": []
		},
		"copy": {
			"comment": "name < template",
			"match": "^\\s*(?![>\\-#\\\\|`])([^<=:]+?)\\s*(<)\\s*(\\S.*?)\\s*$",
			"captures": {
				"1": { "name": "variable.other.name.eno" },
				"2": { "name": "punctuation.definition.copy.operator.eno" },
				"3": { "name": "variable.other.template.name.eno" }
			},
			"patterns": []
		},
		"entry": {
			"comment": "name = value",
			"match": "^\\s*(?![>\\-#\\\\|`])([^<=:]+?)\\s*(=)\\s*(.+?)?\\s*$",
			"captures": {
				"1": { "name": "variable.other.entry.name.eno" },
				"2": { "name": "punctuation.definition.entry.operator.eno" },
				"3": { "name": "string.unquoted.value.eno" }
			},
			"patterns": []
		},
		"escapedCopy": {
			"comment": "name < template",
			"match": "^\\s*(`+)\\s*((?:(?!\\1).)+)\\s*(\\1)\\s*(<)\\s*(\\S.*?)\\s*$",
			"captures": {
				"1": { "name": "punctuation.definition.escape.begin.operator.eno" },
				"2": { "name": "variable.other.name.eno" },
				"3": { "name": "punctuation.definition.escape.end.operator.eno" },
				"4": { "name": "punctuation.definition.copy.operator.eno" },
				"5": { "name": "variable.other.template.name.eno" }
			},
			"patterns": []
		},
		"escapedEntry": {
			"comment": "`name` = value",
			"match": "^\\s*(`+)\\s*((?:(?!\\1).)+)\\s*(\\1)\\s*(=)\\s*(.+?)?\\s*$",
			"captures": {
				"1": { "name": "punctuation.definition.escape.begin.operator.eno" },
				"2": { "name": "variable.other.entry.name.eno" },
				"3": { "name": "punctuation.definition.escape.end.operator.eno" },
